ns1:np-LEV1999-C1-10.1242_dev.126.21.4703-assertion {
    ns1:claim-LEV1999-C1 a ns1:Assertion ;
        dcterms:description "Gap-junctional communication is required early in chick embryos to establish left-sided Shh/Nodal asymmetry; blocking Cx43-mediated coupling causes bilateral/symmetrical expression, indicating a pathway upstream of nodal flow/cilia." ;
        ns4:citesAsEvidence <https://doi.org/10.1242/dev.126.21.4703> ;
        ns1:context "Gallus gallus; pharmacologic blockade (lindane), Cx43 antisense/antibody; Shh/Nodal expression sidedness at HH stage 2–3." ;
        ns1:hasAssessment ns1:assessment-LEV1999-C1 ;
        ns3:hasEvidence ns1:evidence-LEV1999-C1-E1,
            ns1:evidence-LEV1999-C1-E2 .
}

