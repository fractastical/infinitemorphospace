    ns3:claim-PAI2017-C1 a ns3:Assertion ;
        dcterms:description "HCN4 channel function is required for very early (pre-nodal) events establishing LR patterning; perturbing HCN4 disrupts situs and LR gene expression independently of the Nodal/Lefty cascade." ;
        ns4:citesAsEvidence <https://doi.org/10.1242/bio.025957> ;
        ns3:context "Xenopus laevis embryos; HCN4 morpholino/antagonists; situs scoring and LR gene expression (Xnr-1/Pitx2); pre-cilia stages." ;
        ns3:hasAssessment ns3:assessment-PAI2017-C1 ;
        ns2:hasEvidence ns3:evidence-PAI2017-C1-E1,
            ns3:evidence-PAI2017-C1-E2 .
}

