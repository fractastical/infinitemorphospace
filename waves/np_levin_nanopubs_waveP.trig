ns1:np-PAI2022-C1-10.1111_wrr.13032-assertion {
    ns1:claim-PAI2022-C1 a ns1:Assertion ;
        dcterms:description "Induced HCN2 expression locally or at a distance rescues CNS (brain, eye) and non-CNS (heart, gut) teratogenesis across etiologies (nicotine, ethanol, Notch); small-molecule activators also work." ;
        ns2:citesAsEvidence <https://doi.org/10.1111/wrr.13032> ;
        ns1:context "Xenopus laevis; multi-organ teratogenesis; HCN2 misexpression/activators; local vs distant rescue; timing windows." ;
        ns1:hasAssessment ns1:assessment-PAI2022-C1 ;
        ns3:hasEvidence ns1:evidence-PAI2022-C1-E1,
            ns1:evidence-PAI2022-C1-E2 .
}

