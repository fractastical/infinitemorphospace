        dcterms:description "Hyperpolarization of oncogene-bearing cells suppresses tumor-like structures (ITLS), while further depolarization increases ITLS incidence." ;
        ns4:citesAsEvidence <https://doi.org/10.1242/dmm.010835> ;
        ns1:context "Xenopus laevis embryos; oncogene Xrel3; Kir4.1 (K+) and GlyRF99A (Cl-) hyperpolarizing channels; ivermectin depolarization; ITLS count at stage 35." ;
        ns1:hasAssessment ns1:assessment-CL2013-C1 ;
        ns3:hasEvidence ns1:evidence-CL2013-C1-E1,
            ns1:evidence-CL2013-C1-E2 .
}

