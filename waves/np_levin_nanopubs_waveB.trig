ns1:np-AML2007-C1-10.1242_dev.02812-assertion {
    ns1:claim-AML2007-C1 a ns1:Assertion ;
        dcterms:description "V-ATPase-driven H+ flux is necessary for tail regeneration, and a concanamycin-insensitive H+ pump (PMA) is sufficient to rescue regeneration under V-ATPase inhibition." ;
        ns4:citesAsEvidence <https://doi.org/10.1242/dev.02812> ;
        ns1:context "Xenopus laevis tadpole; amputation at stage 41; V-ATPase inhibition (concanamycin) and dominant-negative E-subunit; PMA H+ pump rescue; regeneration index (RI) at 7 dpa." ;
        ns1:hasAssessment ns1:assessment-AML2007-C1 ;
        ns3:hasEvidence ns1:evidence-AML2007-C1-E1,
            ns1:evidence-AML2007-C1-E2,
            ns1:evidence-AML2007-C1-E3,
            ns1:evidence-AML2007-C1-E4 .
}

